<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import com.jfoenix.controls.*?>
<?import javafx.scene.effect.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.UsuariosController">
   <children>
      <Pane layoutX="86.0" layoutY="68.0" prefHeight="264.0" prefWidth="429.0" style="-fx-background-color: #FFFFFF;">
         <effect>
            <DropShadow spread="0.31" />
         </effect>
         <children>
            <VBox layoutX="-2.0" prefHeight="253.0" prefWidth="429.0">
               <children>
                  <JFXTextField fx:id="txtNome" disable="true" labelFloat="true" onKeyPressed="#eventoKeyPressed" promptText="Nome">
                     <VBox.margin>
                        <Insets bottom="3.0" left="10.0" right="10.0" top="20.0" />
                     </VBox.margin>
                  </JFXTextField>
                  <JFXTextField fx:id="txtUsuario" disable="true" labelFloat="true" onKeyPressed="#eventoKeyPressed" promptText="Usuario">
                     <VBox.margin>
                        <Insets bottom="3.0" left="10.0" right="10.0" top="10.0" />
                     </VBox.margin>
                  </JFXTextField>
                  <JFXPasswordField fx:id="txtSenha" disable="true" labelFloat="true" onKeyPressed="#eventoKeyPressed" promptText="Senha">
                     <VBox.margin>
                        <Insets bottom="3.0" left="10.0" right="10.0" top="10.0" />
                     </VBox.margin>
                  </JFXPasswordField>
                  <JFXPasswordField fx:id="txtConfSenha" disable="true" labelFloat="true" onKeyPressed="#eventoKeyPressed" promptText="Confirmar Senha">
                     <VBox.margin>
                        <Insets bottom="3.0" left="10.0" right="10.0" top="10.0" />
                     </VBox.margin>
                  </JFXPasswordField>
                  <Label fx:id="lblStatus" text="Status" visible="false">
                     <VBox.margin>
                        <Insets bottom="10.0" left="20.0" right="10.0" top="10.0" />
                     </VBox.margin>
                  </Label>
                  <HBox prefHeight="27.0" prefWidth="429.0">
                     <children>
                        <JFXButton fx:id="btnSalvar" onAction="#eventoSalvar" prefWidth="90.0" text="Novo">
                           <HBox.margin>
                              <Insets left="5.0" right="5.0" />
                           </HBox.margin>
                        </JFXButton>
                        <JFXButton fx:id="btnAlterar" onAction="#eventoAlterar" prefWidth="90.0" text="Alterar">
                           <HBox.margin>
                              <Insets left="5.0" right="5.0" />
                           </HBox.margin>
                        </JFXButton>
                        <JFXButton fx:id="btnExcluir" onAction="#eventoExcluir" prefWidth="90.0" text="Excluir">
                           <HBox.margin>
                              <Insets left="5.0" right="5.0" />
                           </HBox.margin>
                        </JFXButton>
                        <JFXButton fx:id="btnCancelar" onAction="#eventoCancelar" prefWidth="90.0" text="Cancelar">
                           <HBox.margin>
                              <Insets left="5.0" right="5.0" />
                           </HBox.margin>
                        </JFXButton>
                        <JFXButton fx:id="btnSair" onAction="#eventoSair" prefWidth="90.0" text="Sair">
                           <HBox.margin>
                              <Insets left="5.0" right="5.0" />
                           </HBox.margin>
                        </JFXButton>
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </Pane>
   </children>
</AnchorPane>
